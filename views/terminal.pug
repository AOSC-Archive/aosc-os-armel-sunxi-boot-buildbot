doctype html

mixin include-css(url)
  link(rel="stylesheet", href=url)

mixin include-js(url)
  script(src=url)

html(lang='en')
  head
    meta(charset='utf-8')
    title AOSC Insomnia Builder Terminal

append javascript
  +include-js("/js/hterm_all.min.js")
  +include-js("https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.1/jquery.min.js")
  +include-js("/js/aosc_insomnia.js")
  +include-js("https://cdn.socket.io/socket.io-1.4.5.js")
  script.
      $(function () {
        AOSCHypoxia.startConsole();
      });

block body
  nav(style="color: white;background-color: black;")
    .container
      block nav
        span(id="status") Loading...

  div(id="terminal", style="position: absolute; width: 100%; height: 95%")
